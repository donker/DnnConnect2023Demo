        "**/*.jpeg",
        "**/*.gif",
        "**/*.eot",
        "**/*.svg",
        "**/*.ttf",
        "**/*.woff",
        "**/*.woff2",
        "*.txt"
      ],
      "zipName": "Connect.Agenda"
    }
