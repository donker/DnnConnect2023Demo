{
  "name": "agenda",
  "version": "0.0.1",
  "description": "Agenda module",
  "dnn": {
    "projectFolders": [
      "Server/Agenda"
    ],
    "owner": {
      "name": "Peter Donker",
      "organization": "DNN Connect",
      "url": "dnn-connect.org",
      "email": "peter@bring2mind.net"
    },
    "pathsAndFiles": {
